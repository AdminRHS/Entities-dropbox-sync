{
  "schema_version": "1.0",
  "last_updated": "2025-11-21",
  "description": "Validation schemas for all ENTITIES master lists. Used by PMT-075 (Data Integrity Manager) for schema validation.",
  "schemas": {
    "Actions": {
      "file": "LIBRARIES/Actions/actions_master.json",
      "id_pattern": "ACT-\\d{3}",
      "fields": {
        "id": {
          "type": "string",
          "required": true,
          "pattern": "^ACT-\\d{3}$",
          "description": "Unique action ID (ACT-001 to ACT-429)"
        },
        "action": {
          "type": "string",
          "required": true,
          "description": "Action verb (e.g., create, analyze, extract)"
        },
        "type": {
          "type": "string",
          "required": true,
          "enum": ["A", "B", "C", "D", "E", "F", "G"],
          "description": "Action category: A=Creation, B=Modification, C=Analysis, D=Organization, E=Communication, F=Agentic, G=Data_Operations"
        },
        "description": {
          "type": "string",
          "required": false,
          "description": "Detailed description of the action"
        },
        "variants": {
          "type": "array",
          "required": false,
          "description": "Alternative forms of the action verb"
        },
        "last_updated": {
          "type": "string",
          "required": false,
          "pattern": "^\\d{4}-\\d{2}-\\d{2}$",
          "description": "Last update date (YYYY-MM-DD)"
        }
      }
    },
    "Objects": {
      "file": "LIBRARIES/Objects/objects_master.json",
      "id_pattern": "OBJ-\\d{3}",
      "fields": {
        "id": {
          "type": "string",
          "required": true,
          "pattern": "^OBJ-\\d{3}$"
        },
        "object": {
          "type": "string",
          "required": true,
          "description": "Object noun (e.g., file, data, report)"
        },
        "category": {
          "type": "string",
          "required": false,
          "description": "Object category"
        },
        "description": {
          "type": "string",
          "required": false
        },
        "last_updated": {
          "type": "string",
          "required": false,
          "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
        }
      }
    },
    "Tools": {
      "file": "LIBRARIES/Tools/Tools_Master_List.csv",
      "id_pattern": "TOL-\\d{3}",
      "fields": {
        "tool_id": {
          "type": "string",
          "required": true,
          "pattern": "^TOL-\\d{3}$"
        },
        "tool_name": {
          "type": "string",
          "required": true,
          "description": "Tool name (e.g., Claude AI, n8n)"
        },
        "category": {
          "type": "string",
          "required": false,
          "enum": ["AI_Platform", "Automation", "Development", "Design", "Productivity", "Database", "Integration"]
        },
        "department": {
          "type": "string",
          "required": false,
          "enum": ["AID", "HRM", "DEV", "DGN", "VID", "LGN", "SLS", "MKT", "SMM", "FIN", "CNT", "Multi", "OPS"]
        },
        "url": {
          "type": "string",
          "required": false,
          "description": "Tool website URL"
        },
        "description": {
          "type": "string",
          "required": false
        },
        "status": {
          "type": "string",
          "required": true,
          "enum": ["Active", "Deprecated", "Evaluating"],
          "default": "Active"
        },
        "last_updated": {
          "type": "string",
          "required": false,
          "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
        }
      }
    },
    "Responsibilities": {
      "file": "LIBRARIES/Responsibilities/responsibilities_master.json",
      "id_pattern": "RSP-\\d{3}",
      "fields": {
        "id": {
          "type": "string",
          "required": true,
          "pattern": "^RSP-\\d{3}$"
        },
        "responsibility": {
          "type": "string",
          "required": true,
          "description": "Action + Object pair (e.g., Extract Data, Create Report)"
        },
        "action_id": {
          "type": "string",
          "required": true,
          "pattern": "^ACT-\\d{3}$",
          "description": "Referenced action ID"
        },
        "object_id": {
          "type": "string",
          "required": true,
          "pattern": "^OBJ-\\d{3}$",
          "description": "Referenced object ID"
        },
        "department": {
          "type": "string",
          "required": false,
          "enum": ["AID", "HRM", "DEV", "DGN", "VID", "LGN", "SLS", "MKT", "SMM", "FIN", "CNT", "Multi", "OPS"]
        },
        "tools_used": {
          "type": "array",
          "required": false,
          "description": "Array of TOL-### IDs"
        },
        "last_updated": {
          "type": "string",
          "required": false,
          "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
        }
      }
    },
    "Professions": {
      "file": "LIBRARIES/Professions/professions_master.json",
      "id_pattern": "PRF-\\d{3}",
      "fields": {
        "id": {
          "type": "string",
          "required": true,
          "pattern": "^PRF-\\d{3}$"
        },
        "profession": {
          "type": "string",
          "required": true,
          "description": "Profession name (e.g., AI Engineer, Data Analyst)"
        },
        "department": {
          "type": "string",
          "required": false,
          "enum": ["AID", "HRM", "DEV", "DGN", "VID", "LGN", "SLS", "MKT", "SMM", "FIN", "CNT", "Multi", "OPS"]
        },
        "key_responsibilities": {
          "type": "array",
          "required": false,
          "description": "Array of RSP-### IDs"
        },
        "tools": {
          "type": "array",
          "required": false,
          "description": "Array of TOL-### IDs"
        }
      }
    },
    "Projects": {
      "file": "TASK_MANAGERS/TSM-001_Project_Templates/Project_Templates_Master_List.csv",
      "id_pattern": "PRT-\\d{3}",
      "fields": {
        "New_ID": {
          "type": "string",
          "required": true,
          "pattern": "^PRT-\\d{3}$"
        },
        "Entity_Type": {
          "type": "string",
          "required": true,
          "enum": ["Project"],
          "default": "Project"
        },
        "Current_ID": {
          "type": "string",
          "required": false,
          "description": "Legacy ID if migrated"
        },
        "Name": {
          "type": "string",
          "required": true,
          "description": "Project name with underscores (e.g., AI_Tutorial_Research)"
        },
        "Description": {
          "type": "string",
          "required": true
        },
        "Department": {
          "type": "string",
          "required": true,
          "enum": ["AID", "HRM", "DEV", "DGN", "VID", "LGN", "SLS", "MKT", "SMM", "FIN", "CNT", "Multi", "OPS"]
        },
        "File_Path": {
          "type": "string",
          "required": true,
          "description": "Relative path from ENTITIES/"
        },
        "Status": {
          "type": "string",
          "required": true,
          "enum": ["Active", "In Progress", "Paused", "Completed", "Deprecated"],
          "default": "Active"
        },
        "Last_Updated": {
          "type": "string",
          "required": false,
          "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
        }
      }
    },
    "Milestones": {
      "file": "TASK_MANAGERS/TSM-002_Milestone_Templates/Milestone_Templates_Master_List.csv",
      "id_pattern": "MLT-\\d{3}",
      "fields": {
        "New_ID": {
          "type": "string",
          "required": true,
          "pattern": "^MLT-\\d{3}$"
        },
        "Entity_Type": {
          "type": "string",
          "required": true,
          "enum": ["Milestone"],
          "default": "Milestone"
        },
        "Name": {
          "type": "string",
          "required": true
        },
        "Description": {
          "type": "string",
          "required": true
        },
        "project_template_id": {
          "type": "string",
          "required": true,
          "pattern": "^PRT-\\d{3}$",
          "description": "Parent project ID"
        },
        "Department": {
          "type": "string",
          "required": false,
          "enum": ["AID", "HRM", "DEV", "DGN", "VID", "LGN", "SLS", "MKT", "SMM", "FIN", "CNT", "Multi", "OPS"]
        },
        "Status": {
          "type": "string",
          "required": true,
          "enum": ["Active", "In Progress", "Paused", "Completed", "Deprecated"]
        },
        "Last_Updated": {
          "type": "string",
          "required": false,
          "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
        }
      }
    },
    "Tasks": {
      "file": "TASK_MANAGERS/TSM-003_Task_Templates/Task_Templates_Master_List.csv",
      "id_pattern": "TSK-\\d{3}",
      "fields": {
        "New_ID": {
          "type": "string",
          "required": true,
          "pattern": "^TSK-\\d{3}$"
        },
        "Entity_Type": {
          "type": "string",
          "required": true,
          "enum": ["Task"],
          "default": "Task"
        },
        "Name": {
          "type": "string",
          "required": true,
          "description": "Task name with underscores"
        },
        "Description": {
          "type": "string",
          "required": true
        },
        "milestone_template_id": {
          "type": "string",
          "required": true,
          "pattern": "^MLT-\\d{3}$",
          "description": "Parent milestone ID"
        },
        "Department": {
          "type": "string",
          "required": true,
          "enum": ["AID", "HRM", "DEV", "DGN", "VID", "LGN", "SLS", "MKT", "SMM", "FIN", "CNT", "Multi", "OPS"]
        },
        "tools_referenced": {
          "type": "array",
          "required": false,
          "description": "Array of TOL-### IDs"
        },
        "Status": {
          "type": "string",
          "required": true,
          "enum": ["Active", "In Progress", "Paused", "Completed", "Deprecated"]
        },
        "Last_Updated": {
          "type": "string",
          "required": false,
          "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
        }
      }
    },
    "Steps": {
      "file": "TASK_MANAGERS/TSM-004_Step_Templates/Step_Templates_Master_List.csv",
      "id_pattern": "STT-\\d{3}",
      "fields": {
        "New_ID": {
          "type": "string",
          "required": true,
          "pattern": "^STT-\\d{3}$"
        },
        "Entity_Type": {
          "type": "string",
          "required": true,
          "enum": ["Step"],
          "default": "Step"
        },
        "Name": {
          "type": "string",
          "required": true
        },
        "Description": {
          "type": "string",
          "required": true
        },
        "task_template_id": {
          "type": "string",
          "required": true,
          "pattern": "^TSK-\\d{3}$",
          "description": "Parent task ID"
        },
        "Status": {
          "type": "string",
          "required": true,
          "enum": ["Active", "In Progress", "Paused", "Completed", "Deprecated"]
        }
      }
    },
    "ChecklistTemplates": {
      "file": "TASK_MANAGERS/TSM-005_Checklist_Items/Checklist_Items_Master_List.csv",
      "id_pattern": "CHT-\\d{3}",
      "fields": {
        "New_ID": {
          "type": "string",
          "required": true,
          "pattern": "^CHT-\\d{3}$"
        },
        "Entity_Type": {
          "type": "string",
          "required": true,
          "enum": ["Checklist_Template"],
          "default": "Checklist_Template"
        },
        "Name": {
          "type": "string",
          "required": true
        },
        "Description": {
          "type": "string",
          "required": true
        },
        "Department": {
          "type": "string",
          "required": false,
          "enum": ["AID", "HRM", "DEV", "DGN", "VID", "LGN", "SLS", "MKT", "SMM", "FIN", "CNT", "Multi", "OPS"]
        },
        "File_Path": {
          "type": "string",
          "required": true
        },
        "Status": {
          "type": "string",
          "required": true,
          "enum": ["Active", "Deprecated"]
        }
      }
    },
    "ChecklistItems": {
      "file": "TASK_MANAGERS/TSM-005_Checklist_Items/CHT-*.json",
      "id_pattern": "(CHK-\\d{3}|Checklist-Item-\\d{3})",
      "fields": {
        "checklist_item_id": {
          "type": "string",
          "required": true,
          "pattern": "^(CHK-\\d{3}|Checklist-Item-\\d{3})$",
          "description": "Checklist item ID (CHK-101 or Checklist-Item-001)"
        },
        "item_name": {
          "type": "string",
          "required": true
        },
        "entity_type": {
          "type": "string",
          "required": true,
          "enum": ["TASK_MANAGERS"]
        },
        "sub_entity": {
          "type": "string",
          "required": true,
          "enum": ["Checklist_Item"]
        },
        "associated_project": {
          "type": "string",
          "required": false,
          "description": "Parent project reference"
        },
        "associated_milestone": {
          "type": "string",
          "required": false,
          "description": "Parent milestone reference"
        },
        "associated_task": {
          "type": "string",
          "required": false,
          "description": "Parent task reference"
        },
        "phase": {
          "type": "number",
          "required": false,
          "description": "Phase number (1, 2, 3, etc.)"
        },
        "status": {
          "type": "string",
          "required": true,
          "enum": ["pending", "in_progress", "completed", "blocked"]
        },
        "required": {
          "type": "boolean",
          "required": true,
          "default": true
        },
        "item_type": {
          "type": "string",
          "required": true,
          "enum": ["deliverable", "validation", "process", "documentation"]
        },
        "category": {
          "type": "string",
          "required": false,
          "enum": ["quantitative", "technical", "process", "documentation", "quality"]
        },
        "acceptance_criteria": {
          "type": "array",
          "required": false,
          "description": "Array of acceptance criteria strings"
        },
        "quality_criteria": {
          "type": "array",
          "required": false,
          "description": "Array of quality criteria strings"
        }
      }
    },
    "Prompts": {
      "file": "PROMPTS/DATA_FIELDS/PMT_Master_List.csv",
      "id_pattern": "PMT-\\d{3}",
      "fields": {
        "PMT_ID": {
          "type": "string",
          "required": true,
          "pattern": "^PMT-\\d{3}$"
        },
        "Entity_Type": {
          "type": "string",
          "required": true,
          "enum": ["Prompt"],
          "default": "Prompt"
        },
        "Name": {
          "type": "string",
          "required": true
        },
        "Description": {
          "type": "string",
          "required": true
        },
        "Category": {
          "type": "string",
          "required": true,
          "enum": ["CORE", "VIDEO", "REPORTS", "TAXONOMY", "RESEARCH", "HR", "WORKFLOW", "AUTOMATION", "UTILITIES", "SYSTEM", "DATA_ARCHITECTURE"]
        },
        "Department": {
          "type": "string",
          "required": true,
          "enum": ["AID", "HRM", "DEV", "DGN", "VID", "LGN", "SLS", "MKT", "SMM", "FIN", "CNT", "Multi", "OPS"]
        },
        "File_Path": {
          "type": "string",
          "required": true
        },
        "Status": {
          "type": "string",
          "required": true,
          "enum": ["Active", "Utility", "Deprecated"]
        },
        "Version": {
          "type": "string",
          "required": true,
          "description": "Version number (e.g., 1.0, 2.1)"
        },
        "Last_Updated": {
          "type": "string",
          "required": true,
          "pattern": "^\\d{4}-\\d{2}-\\d{2}$"
        }
      }
    }
  },
  "validation_rules": {
    "global": {
      "id_format": "Must match pattern {PREFIX}-{3_DIGITS} (e.g., TSK-001, TOL-042)",
      "name_format": "Use underscores not spaces for file-based names (e.g., Process_Task_Files)",
      "status_values": ["Active", "In Progress", "Paused", "Completed", "Deprecated"],
      "department_codes": ["AID", "HRM", "DEV", "DGN", "VID", "LGN", "SLS", "MKT", "SMM", "FIN", "CNT", "Multi", "OPS"],
      "date_format": "YYYY-MM-DD (ISO 8601)"
    },
    "cross_references": {
      "TSK_to_MLT": "Task.milestone_template_id must exist in Milestones.New_ID",
      "MLT_to_PRT": "Milestone.project_template_id must exist in Projects.New_ID",
      "STT_to_TSK": "Step.task_template_id must exist in Tasks.New_ID",
      "RSP_to_ACT": "Responsibility.action_id must exist in Actions.id",
      "RSP_to_OBJ": "Responsibility.object_id must exist in Objects.id",
      "CHK_to_Parent": "ChecklistItem.associated_task should reference valid task"
    },
    "hierarchy": {
      "PRT": {
        "children": "MLT",
        "min_children": 0,
        "description": "Projects contain Milestones"
      },
      "MLT": {
        "parent": "PRT",
        "children": "TSK",
        "min_children": 0,
        "description": "Milestones belong to Projects, contain Tasks"
      },
      "TSK": {
        "parent": "MLT",
        "children": "STT",
        "min_children": 0,
        "description": "Tasks belong to Milestones, contain Steps"
      },
      "STT": {
        "parent": "TSK",
        "children": "CHK",
        "min_children": 0,
        "description": "Steps belong to Tasks, may have Checklist Items"
      },
      "CHT": {
        "children": "CHK",
        "description": "Checklist Templates contain Checklist Items"
      }
    }
  },
  "usage": {
    "purpose": "This schema registry is used by PMT-075 (Data Integrity Manager) to validate all master lists",
    "validation_command": "python validate_entity.py --entity Actions --schema Entity_Schema_Registry.json",
    "integration_prompts": ["PMT-075", "PMT-076", "PMT-079"],
    "update_frequency": "Update when new entity types added or schema changes"
  }
}
